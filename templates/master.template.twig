  <body class="{% block pageClass %}{% endblock %}">
    <div class="body-block bg-color-1"></div>
    <div class="body-block-2"></div>
    {% set nav = 0 %}
    {% include "common/logo.twig" with {"id": "primaryLogo", "uniqueClass": "primary-logo"} %}
    {% block sideNavBlock %}
      {% include "common/side-navigation.twig" with { "data": 0 } %}
    {% endblock %}

    {% block pageBespokeContent %}{% endblock %}

    <div id="contentArea" class="content-area">
      {% block body %}{% endblock %}
    </div>

    {% include "common/tree.twig" %}

    <div style="display: none;">
      <div id="ninja" class="ninja"><img src="/images/pocketninja-medium.png"></div>
    </div>
    <script src="bundle.js?201803110922"></script>
  </body>
</html>