<div class="portfolio-gallery">
  <h5 class="portfolio-section-title">{{ cat.title }}</h5>
  <ul class="portfolio-list">
    {% for item in gallery %}
      <li class="portfolio-list-item" data-project-title="{{item.title}}">
        <img src="{{ item.img.small }}">
      </li>
    {% endfor %}
  </ul>
</div>